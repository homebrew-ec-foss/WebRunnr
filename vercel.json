{
  "version": 2,
  "name": "webrunnr",
  "builds": [
    {
      "src": "packages/playground/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/playground/dist/$1"
    }
  ],
  "functions": {},
  "outputDirectory": "packages/playground/dist"
}
